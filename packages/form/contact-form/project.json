{
  "name": "form-contact-form",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/form/contact-form/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/form/contact-form",
        "main": "packages/form/contact-form/src/index.ts",
        "tsConfig": "packages/form/contact-form/tsconfig.lib.json",
        "assets": ["packages/form/contact-form/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/form/contact-form/npm && npm publish"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/form/contact-form/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/form/contact-form"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/form/contact-form"
      }
    }
  },
  "tags": []
}
